<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pcwk.ctrl.notice">
    <select id="doNoticeSelect" resultType="NoticeVO">
        SELECT  n_num AS nNum,
                        n_title AS nTitle,
                        n_content AS nContent,
                        n_name AS nName,
                        n_regdt AS nRegdt
         FROM notice
    </select>
</mapper>